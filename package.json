{
  "name": "hdr-exr-360-viewer",
  "displayName": "HDR/EXR 360 Viewer",
  "description": "Professional 360-degree viewer for HDR and EXR files with real-time exposure control and WebGL acceleration",
  "version": "0.0.20",
  "publisher": "Mehanth",
  "license": "MIT",
  "author": {
    "name": "Mehanth",
    "url": "https://github.com/Itz-mehanth"
  },
  "homepage": "https://github.com/Itz-mehanth/VSCE#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Itz-mehanth/VSCE"
  },
  "bugs": {
    "url": "https://github.com/Itz-mehanth/VSCE/issues"
  },
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "hdr",
    "exr",
    "viewer",
    "360",
    "image",
    "environment",
    "tone-mapping",
    "webgl",
    "three.js",
    "visualization"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "hdrExr360Viewer.viewer",
        "displayName": "HDR/EXR 360 Viewer",
        "selector": [
          {
            "filenamePattern": "*.hdr"
          },
          {
            "filenamePattern": "*.exr"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && npm run build:webview && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "build:webview": "node esbuild-webview.js",
    "package": "npm run check-types && npm run lint && npm run build:webview && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "esbuild": "^0.27.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}